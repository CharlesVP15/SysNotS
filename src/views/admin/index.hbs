      <link rel="stylesheet" href="/css/bootstrap.min.css" type="text/css">
      <link rel="stylesheet" href="/sweetalert2/sweetalert2.min.css" type="text/css">
      <link rel="stylesheet" href="/css/estilo_g.css" type="text/css">
      <link rel="stylesheet" href="/css/letrasFormat.css" type="text/css">
      <link rel="stylesheet" href="/css/designButton.css" type="text/css">
      <style>
         input {
            opacity: 1;
         }
      </style>
   </head>

   <body>

      {{> navigationNotes}}

      <img class="wave" src="/img/w.png">
      <img class="wave3" src="/img/Innova Tech'S.png">
      <img class="wave4" src="/img/SysNot'S2.png">
      
      <div class="container">
         
         <br>
         {{> messages}}
         {{> errors}}
         <br>

         <div class="row">
            <div class="col-md-10 mx-auto">
               <form class="was-validated" action="/registerProfileAdmin" method="post">
                  <div class="card border-primary mb-3" style="opacity: 0.9;">
                     <div class="card-header bg-primary text-white">
                        <b style="font-size: 20px; text-transform: uppercase;">&nbsp;&nbsp;REGISTRAR Administrador o Usuario</b>
                     </div>

                     <div class="card-body text-primary">
                        <div class="form-row mb-3">
                           <img class="mx-auto" src="/img/avatar_azul.svg" height="120px" width="120px">
                        </div>

                        <div class="form-row">
                           <div class="col-md-10">
                              <h5 class="h5">Datos Personales - Usuario</h5>
                           </div>
                        </div>

                        <div class="form-row">
                           <div class="col-md-3 mb-3">
                              <b><label># de Identificación :</label></b>
                              <input type="text" class="form-control" id="cedulaAd" name="cedulaAd" onkeypress="return numeros(event)" onkeyUp="return CedulaAd(this);" maxlength="10" value="{{cedulaAd}}" required autofocus>
                           </div>

                           <div class="col-md-4 mb-3">
                              <b><label>Apellidos :</label></b>
                              <input type="text" class="form-control" id="apellidosAd" name="apellidosAd" onkeypress="return letra(event)" value="{{apellidosAd}}" required>
                           </div>

                           <div class="col-md-4 mb-3">
                              <b><label>Nombres :</label></b>
                              <input type="text" class="form-control" id="nombresAd" name="nombresAd" onkeypress="return letra(event)" value="{{nombresAd}}" required>
                           </div>
                           
                           <div class="col-md-1 mb-3">
                              <b><label>Edad :</label></b>
                              <input type="text" class="form-control" id="edadAd" name="edadAd" onkeypress="return numeros(event)" maxlength="3" pattern="[0-9]{1-3}" value="{{edadAd}}" required>
                           </div>
                        </div>

                        <div class="form-row">
                           <div class="col-md-3 mb-3">
                              <b><label>Sexo : </label></b> 
                              <select name="sexoAd" id="sexoAd" required class="custom-select">
                                 <option disabled selected value="">Seleccionar...</option>
                                 <option value="Masculino">Masculino</option>
                                 <option value="Femenino">Femenino</option>
                                 <option value="NDefinido">No definido</option>
                              </select>
                           </div>

                           <div class="col-md-4 mb-3">
                              <b><label>Correo :</label></b>
                              <input type="email" class="form-control" id="emailAd" name="emailAd" onkeypress="return emailD(event)" style="text-transform: lowercase;" value="{{emailAd}}" required>
                           </div>

                           <div class="col-md-3 mb-3">
                              <b><label>Privilegio : </label></b> 
                              <select name="privilegioAd" id="privilegioAd" required class="custom-select">
                                 <option disabled selected value="">Seleccionar...</option>
                                 <option value="Usuario">Usuario</option>
                                 <option value="Admin">Administrador</option>
                              </select>
                           </div>
                        </div>

                        <div class="form-row">
                           <div class="col-md-10">
                              <h5 class="h5">Datos de Sesión</h5>
                           </div>
                        </div>

                        <div class="form-row">
                           <div class="col-md-4 mb-3">
                              <b><label>User :</label></b>
                              <input type="hidden" class="form-control" id="profileAd" name="profileAd" value="ui.svg">

                              <input type="text" class="form-control" id="userAd" name="userAd" minlength="7" maxlength="15" pattern="[a-zA-Z]{7-15}" onkeypress="return userU(event)" placeholder="Ej. Usuario Charl123" value="{{userAd}}" required>
                           </div>

                           <div class="col-md-4 mb-3">
                              <b><label>Password :</label></b>
                              <input type="password" class="form-control" id="passwordAd" name="passwordAd" minlength="8" placeholder="Minimo 6 caracteres" required>
                           </div>

                           <div class="col-md-4 mb-3">
                              <b><label>Confirm Password :</label></b>
                              <input type="password" class="form-control" id="conf_passwordAd" name="conf_passwordAd" minlength="8" placeholder="Confirmar Contraseña" required>
                           </div>
                        </div>
                        
                     </div>
                  </div>
                  
                  <button class="btn float-right col-md-4" id="Enviar" type="submit" disabled>
                     &nbsp;&nbsp;&nbsp;
                     <i class="fas fa-user-plus"></i>
                     &nbsp;&nbsp;
                     Registrarse...
                     &nbsp;&nbsp;&nbsp;
                  </button>
               </form>
            </div>
         </div>

         <br><br>

      </div>

      <script src="/js/jquery.min.js"></script>
      <script src="/js/popper.min.js"></script>
      <script src="/js/bootstrap.min.js"></script>
      <script src="/sweetalert2/sweetalert2.min.js"></script>
      <script src="/css/fontawesome/js/all.min.js"></script>
      <script src="/js/validation.js"></script>

